    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.0",
    "@types/rimraf": "^3.0.0",
    "@types/shelljs": "^0.8.7",
    "commitizen": "^4.1.2",
    "conventional-changelog-cli": "^2.0.34",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^6.8.0",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "lint-staged": "^10.2.2",
    "np": "^6.2.3",
    "prettier": "^2.0.4",
    "shelljs": "^0.8.4",
    "ts-jest": "^25.4.0",
    "vue": "^3.0.0-beta.16"
  }
